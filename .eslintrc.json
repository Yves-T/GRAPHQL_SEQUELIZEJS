{
    "extends": "eslint-config-google",
    "parser": "babel-eslint",
    "ecmaFeatures": {
        "jsx": true,
        "modules": false
    },
    "rules": {
        "arrow-parens": ["error", "as-needed"],
        "space-before-function-paren": ["error", {
            "anonymous": "always",
            "named": "always",
            "asyncArrow": "ignore"
        }]
    }
}
